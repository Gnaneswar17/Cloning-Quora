<% for(var i of comment_obj){ %>
    <b>
        Copied Comment : <%= i[0].comment %> <br>
    </b>
    Username : <b>
        <a href="/user/userDetails/?user_id=<%= i[0].user.id %>">
            <%= i[0].user.fname + ' ' + i[0].user.lname %> 
        </a>
        <br>
    </b><br>
    <b>
        Copied From : 
    </b><br>
    <ol>
        <% for(var j of i[1]){ %>
            <li>
                Comment : <%= j[0].comment %> <br>
                Username : 
                <a href="/user/userDetails/?user_id=<%= j[0].user.id %>">
                    <%= j[0].user.fname + ' ' + j[0].user.lname %> 
                </a><br>
                Similarity Percentage : 
                <span style="color: rgb(255, 0, 0);"><%= j[1] %></span>
            </li>
        <% } %>
    </ol>
    <hr>

<% } %>